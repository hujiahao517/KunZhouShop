/* pages/view/profile/profile.wxss */
.profile_noLogged {
  width: 100%;
  height: calc(100vh - 116rpx);
  background-color: #ffffff;
  padding: 0rpx 48rpx;
  box-sizing: border-box;
}

.welcome {
  height: 66rpx;
  font-size: 48rpx;
  font-weight: bold;
  color: #333333;
  margin-top: 44rpx;

}

.short_message {
  font-size: 24rpx;
  color: #999999;
  margin-top: 40rpx;
}

.box {
  display: flex;
  align-items: center;
  color: #999999;
  justify-content: space-between;
  box-sizing: border-box;
  border-bottom: 2rpx solid #E8E8E8;
  width: 100%;
}

.phone {
  padding-bottom: 20rpx;
  height: 74rpx;
  margin-top: 164rpx;
}

.phone_text,
.identifying_code_text {
  font-size: 24rpx;
  font-weight: 400;
  color: #999999;
}

.phone_input {
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
}

.inpit {
  color: #000000;
}

.identifying_code_box {
  height: 90rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #E8E8E8;
}

.identifying_code {
  width: 156rpx;
  height: 60rpx;
  border: 2rpx solid #FF6F2D;
  opacity: 1;
  border-radius: 32rpx;
  text-align: center;
  color: #FF6F2D;
  font-size: 24rpx;
  line-height: 60rpx;
}

.login {
  width: 100% !important;
  height: 88rpx;
  background: linear-gradient(129deg, #FF6F2D 0%, #FF6F2D 100%);
  border-radius: 64rpx;
  margin-top: 222rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tishizhuce{
  font-size: 20rpx;
font-weight: 400;
line-height: 28rpx;
color: #999999;
text-align: center;
margin-top: 40rpx;
}
.other{
  margin: 0rpx 84rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 238rpx;
}
.other_left,.other_right{
  flex: 1;
  height: 0px;
  border-bottom: 1px solid #E8E8E8;
  opacity: 1;
}
.other_center{
  color: #333333;
  font-size: 20rpx;
