
.content{
  width: 694rpx;
height: 582rpx;
background: #FFFFFF;
border-radius: 16rpx;
margin: 20rpx auto 0rpx;
padding: 0rpx 28rpx;
box-sizing: border-box;
}
.content_Q1{
  height: 90rpx;
  border-bottom: 1px solid #E8E8E8;
  display: flex;
  flex-direction: row;
}
.content_Q1_left{
  width: 258rpx;
  font-size: 28rpx;
font-weight: 400;
color: #333333;
display: flex;
align-items: center;

}
.content_Q1_left>text{
  color: #FF6F2D;
}
.content_Q1_right{
  flex: 1;
  display: flex;
  align-items: center;
}
.content_Q1_right>input{
  font-size: 24rpx;
font-weight: 400;
color: #999999;
}
.content_bottom_title{
  color: #FF6F2D;
  font-size: 28rpx;
font-weight: 400;
padding: 26rpx 0rpx 24rpx 0rpx;
}
.content_bottom_content{
  text-align: center;
  width: 152rpx;
  height: 152rpx;
  border: 1rpx currentColor ;
}
.content_bottom_content>image{
  width: 152rpx;
  height: 152rpx;
  display: block;
}
.img_text{
  margin-top: 20rpx;
  font-size: 20rpx;
font-weight: 400;
color: #FF6F2D;
}