<view class="container">
<!-- 添加子账户  -->
  <view class="addcontent">
      <view class="content_Q1">
        <view class="content_Q1_left">手机号</view>
        <view class="content_Q1_right">
          <input type="text" maxlength="11" placeholder-class="inputclass" placeholder="请输入手机号"/>
        </view>
      </view>
      <view class="content_Q1">
        <view class="content_Q1_left">验证码</view>
        <view class="content_Q1_right">
          <input type="6" maxlength="6" placeholder-class="inputclass" 
           placeholder="请输入验证码"/>
        </view>
        <view class="yanzheng">获取验证码</view>
      </view>
  </view>
  <!-- <view class="determine">确定</view> -->
  <view class="btns">
    <view class="btns_left" bindtap="click_son_account">查看子账号</view>
    <view class="btns_right">完成</view>
  </view>
</view>